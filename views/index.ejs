<div class="jumbotron" style="background: #AAAAAA; color: #FFFFFF">
  <h1 class="text-center">
    <% let name = path.basename(__filename, '.ejs'); %>
    <%= name.substring(0, 1).toUpperCase() +
          name.substring(1).toLowerCase();
    %>
  </h1>
</div>
<div>
  <table class="table table-bordered table-sm table-inverse">
    <thead>
      <tr>
        <% for (let i = 0; i < studentPaths.length; i++) { %>
          <th>
            <%= studentPaths[i]
                  .substring(0, 1)
                  .toUpperCase() +

                studentPaths[i]
                  .substring(1);
            %>
          </th>
        <% } %>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < students.length; i++) { %>
        <%- include('modelTemplates/student.ejs', { entry: students[i] }) %>
      <% } %> 
    </tbody>
  </table>
</div>